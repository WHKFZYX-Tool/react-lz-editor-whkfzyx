{
  "name": "react-lz-editor-whkfzyx",
  "version": "1.0.7",
  "description": "An open source react editor based on react-lz-editor and draft-Js and ant design, good support HTML, markdown and Draft Raw format.",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "webpack",
    "typecheck": "flow",
    "dev": "webpack-dev-server --devtool eval --progress --colors --hot --content-base build && lessc src/global/supports/resources/system.less src/global/supports/resources/system.css && lessc src/editor/components.css publish/editor/components.css",
    "release": "webpack -p --progress --colors --config webpack.release.config.js",
    "publish": "babel -d publish/ src/ --no-comments && lessc src/global/supports/resources/system.less publish/global/supports/resources/system.css && lessc src/editor/components.css publish/editor/components.css"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/WHKFZYX-Tool/react-lz-editor-whkfzyx.git"
  },
  "keywords": [
    "react-editor",
    "draft-js",
    "draftjs",
    "react-rich-editor",
    "react-text-editor",
    "react-lz-editor",
    "contenteditable",
    "richtext",
    "editor"
  ],
  "author": "zhougz@mingyuanyun.com",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/WHKFZYX-Tool/react-lz-editor-whkfzyx/issues"
  },
  "homepage": "https://github.com/WHKFZYX-Tool/react-lz-editor-whkfzyx#readme",
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-loader": "^6.2.4",
    "babel-plugin-import": "^1.1.1",
    "babel-plugin-transform-class-properties": "^6.19.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-2": "^6.18.0",
    "css-loader": "^0.23.1",
    "eslint": "^3.12.2",
    "eslint-plugin-react": "^6.8.0",
    "file-loader": "^0.8.5",
    "flow-bin": "^0.37.4",
    "less": "^2.7.2",
    "less-loader": "^2.2.3",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1",
    "hmacsha1": "^1.0.0",
    "md5": "^2.2.1"
  },
  "dependencies": {
    "antd": "^2.8.3",
    "draft-js-whkfzyx": "^0.10.0",
    "immutable": "^3.8.1",
    "js-base64": "^2.1.9",
    "lodash": "^4.17.3",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "synthetic-dom": "^0.2.1",
    "whatwg-fetch": "^2.0.3"
  }
